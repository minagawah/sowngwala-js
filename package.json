{
  "name": "sowngwala-js",
  "version": "0.11.1",
  "private": false,
  "description": "JS library for calculating the sun & moon's position.",
  "main": "dist/sowngwala-$npm_package_version.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "dist/sowngwala-$npm_package_version.js",
    "require": "dist/sowngwala-$npm_package_version.cjs"
  },
  "scripts": {
    "eslint": "eslint --ext js \"{src,src.check}/**/*.js\"",
    "eslint:fix": "eslint --fix --ext js \"{src,src.check,tools}/**/*.js\"",
    "prettier": "prettier --write \"*.{js,md,json}\" \"{src,src.check,tools}/**/*.js\"",
    "lint": "$npm_execpath run prettier && $npm_execpath run eslint:fix",
    "type:check": "tsc -p tsconfig.json",
    "type:generate": "rimraf types && tsc -p tsconfig.generate.json",
    "jsdoc": "rimraf jsdoc && jsdoc -c jsdoc.conf.js -d jsdoc/",
    "test": "jest",
    "build:css": "postcss src.check/styles --dir dist/check",
    "watch:css": "nodemon -x $npm_execpath run build:css -e css -w src.check/styles",
    "watch:js": "webpack serve --config webpack.dev.js",
    "build:js": "webpack --config webpack.prod.js",
    "build:commonjs": "NODE_ENV=commonjs babel src --out-file dist/sowngwala-$npm_package_version.cjs",
    "clean": "rimraf dist",
    "build": "$npm_execpath run clean && $npm_execpath run build:css && $npm_execpath run build:commonjs && $npm_execpath run build:js",
    "stats": "$npm_execpath run clean && webpack --config webpack.prod.js --json > stats.json",
    "dev": "concurrently \"$npm_execpath run watch:css\" \"$npm_execpath run watch:js\""
  },
  "repository": "https://github.com/minagawah/sowngwala-js.git",
  "keywords": [
    "Sun",
    "Moon",
    "Position",
    "Astrology",
    "Astronomy"
  ],
  "author": "minagawah <minag@tokyo800.jp>",
  "license": "(Unlicense OR MIT)",
  "bugs": {
    "url": "https://github.com/minagawah/sowngwala-js/issues"
  },
  "dependencies": {
    "core-js": "^3.35.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@stylistic/eslint-plugin": "^1.5.4",
    "@types/ramda": "^0.29.10",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-preval": "^5.1.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "csv-parse": "^5.5.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdoc-plugin-intersection": "^1.0.4",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "license-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^2.7.7",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-loader": "^8.0.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
